<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Aplikacja - <%= yield(:title) || "Aplikacja" %></title>
    <%= stylesheet_link_tag('application', 'subjects' , :media => 'all') %>
    <%= javascript_include_tag('https://www.google.com/jsapi')%> 
    <script type="text/javascript">
      google.load('visualization', '1', {packages: ['corechart', 'table']});
       google.setOnLoadCallback(drawVisualization);
    </script>
<script type="text/javascript" >


</script>


    <%= javascript_include_tag :defaults ,'effectivity-graph'%> 
  
    <meta name="author" content="Aleksander Gładysz">


  </head>
  <body>
    <div id="header">
      <h1>Simple adder </h1>
    </div>
    <div class="header-pipe-bar">
      
      <% if !flash[:notice].blank? %>
      <div class="notice">
        <%= flash[:notice] %>
      </div>

      <% end %>
    </div>

    <div id="main">
      <div id="content">
        <%= yield %>
      </div>
    </div>
    <div id="footer">
      <p id="copyright">&copy; omninom.local /Aleksander Gładysz</p>
    </div>
  </body>
