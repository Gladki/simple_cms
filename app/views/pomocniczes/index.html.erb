<% title "Typy czynnosci" %>
  <div id="1" class="middle-content">
    
 
  <h1>Typy Predkosci</h1>

<br/>
<table id="tczynnosci" border="1"class="listing" summary="Section list">
    <tr class="header">
      <th>Rodzaj<br/>wózka</th>
      <th>Opis</th>
    
      <th>Kod</th>
      <th>Prędkość <br/>w [km/h]</th>
      <th>Prędkość <br/>w [m/s]</th>
      <th>Prędkość <br/>TMU</th>
      <th>Data utworzenia</th>
    </tr>
 <% @predkosci.each do |predkosc| %>
 <%= render(:partial =>'predkosc', :locals => {:predkosc => predkosc}) %>
          
     <% end %>
   </table>

<div id="insert">
  <p>Dodaj nową prędkość:</p>

 <%= form_for @nowa_predkosc do |f| %>
  
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    
    
  <% end %>

</div>
 </div>
  <div id="2" class="middle-content">
    
 <h1>Dodatki</h1>

<br/>
<table id="tczynnosci" border="1"class="listing" summary="Section list">
    <tr class="header">
      <th>Rodzaj<br/>wózka</th>
      <th>Opis</th>
    
      <th>Kod</th>
      <th>Czas zakrętu<br/>w [sek]</th>
      <th>Czas start/stop<br/>w [sek]</th>
      
      <th>Data utworzenia</th>
    </tr>
 <% @dodatki.each do |dodatek| %>
 <%= render(:partial =>'dodatek', :locals => {:dodatek => dodatek}) %>
          
     <% end %>
   </table>

<div id="insert">
  <p>Dodaj nowy dodatek:</p>

 <%= form_for @nowy_dodatek do |f| %>
  
    <%= render(:partial => 'formd', :locals => {:f => f}) %>
    
    
  <% end %>

</div>
</div>
  <div id="3" class="middle-content">

 <h1>Obszary</h1>

<br/>
<table id="tczynnosci" border="1"class="listing" summary="Section list">
    <tr class="header">
      <th>Miejsce zastosowania</th>
      <th>Magazyn</th>
    
      <th>Kod</th>
      <th>Wartość dodatku</th>
      <th>Opis<br/>obszaru</th>
    
      
      <!-- <th>Data utworzenia</th> -->
    </tr>
 <% @obszary.each do |obszar| %>
 <%= render(:partial =>'obszar', :locals => {:obszar => obszar}) %>
          
     <% end %>
   </table>

<div id="insert" >
  <p>Dodaj nowy obszar:</p>

 <%= form_for @nowy_obszar do |f| %>
  
    <%= render(:partial => 'formo', :locals => {:f => f}) %>
    
    
  <% end %>

</div>
</div>
