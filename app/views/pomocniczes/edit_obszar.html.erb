<%= form_for @obszar do |f| %>
  
  <table summary="Section form fields">
  <tr>
    <th><%= f.label :ob_nazwa, "Miejsce zastosowania: " %></th>
    <td><%= f.text_field :ob_nazwa %></td>
  </tr>
  <tr>
    <th><%= f.label :ob_magazyn, "Nazwa magazynu: " %></th>
    <td><%= f.text_field :ob_magazyn %></td>
  </tr>
   <tr>
    <th><%= f.label :ob_opis, "Opis obszaru: " %></th>
    <td><%= f.text_field :ob_opis %></td>
  </tr>
  <tr>
    <th><%= f.label :ob_kod, "Kod obszaru: " %></th>
    <td><%= f.text_field :ob_kod %></td>
  </tr>
   <tr>
    <th><%= f.label :ob_wartosc, "Wartość dodatku: " %></th>
    <td><%= f.text_field :ob_wartosc, :placeholder => '0.0' %></td>
  </tr>
   
  <tr><td>
    <%= f.hidden_field :ob_aud_us_id, :value => current_user.id %>
    </td>
    </tr>


</table>
     <%= submit_tag("" , :class => "add", :remote => true) %>

    
    
  <% end %>
